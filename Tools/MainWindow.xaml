<Window x:Class="Tools.MainWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" 
        xmlns:local="clr-namespace:Tools" WindowStyle="None" Title="MainWindow" 
        Background="#FF000000" Style="{StaticResource window_Shadow}"
        WindowStartupLocation="CenterScreen" Height="900" Width="1800" AllowsTransparency="True">
    <Grid>
        <Frame Grid.Row="1" Source="{Binding PageUrl}" NavigationUIVisibility="Hidden"></Frame>
    </Grid>

</Window>